                            <images>
                                <image>
                                    <name>
                                        o-ran-sc/nonrtric-plt-a1policymanagementservice:${project.version}</name>
                                    <build>
                                        <contextDir>${basedir}</contextDir>
                                        <dockerFile>Dockerfile</dockerFile>
